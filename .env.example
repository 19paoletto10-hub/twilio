# Twilio Chat App - example environment variables
# Copy to .env and fill before running.

TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_DEFAULT_FROM=+48123456789
TWILIO_MESSAGING_SERVICE_SID=

CHAT_MODE=echo

APP_ENV=dev
APP_DEBUG=true
APP_HOST=0.0.0.0
APP_PORT=3000

DB_PATH=data/app.db
PUBLIC_BASE_URL=
TWILIO_VALIDATE_SIGNATURE=true
# For local tunnels set false temporarily: TWILIO_VALIDATE_SIGNATURE=false
